def int divThree(int num) := =(%(num, 3), 0)
def int divFive(int num) := =(%(num, 5), 0)
def int fizzBuzz(int num) := ; &(divThree(num), divFive(num))
                             ? =(print('FIZZBUZZ'), '')
                             :  ; divThree(num)
                                ? =(print('FIZZ'), '')
                                :  ; divFive(num)
                                   ? =(print('BUZZ'), '')
                                   : print(num)
def int main(int num) := +(fizzBuzz(num), main(+(num, 1)))
main(0)